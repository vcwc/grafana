# START MODULE SETTINGS
export GRAFANA_ENVIRONMENT_GRAFANA_AUTH_ANONYMOUS_ORG_ROLE="${GRAFANA_MODULE_GRAFANA_AUTH_ANONYMOUS_ORG_ROLE}"
